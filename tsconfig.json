{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2022",
		"lib": [
			"es2022",
			"ES2021.String"
		],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"rootDir": "src",
		"jsx": "react",
		"jsxFactory": "jsx",
		"jsxFragmentFactory": "fragment"
	},
	"exclude": ["node_modules", ".vscode-test", "hidden"]
}
